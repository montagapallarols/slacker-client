<div key={c.id}>\n            <p>{c.name}</p>\n            <Link to={`/my-profile/${user.id}/library/${c.name}`}>\n              <Button variant=\"outline-dark\">See list</Button>\n            </Link>\n          </div>\n        );\n      })}\n      <h3>Wishlist</h3>\n      {allCategories?.map((c: any) => {\n        return (\n          <div key={c.id}>\n            <p>{c.name}</p>\n            <Link to={`/my-profile/${user.id}/wishlist/${c.name}`}>\n              <Button variant=\"outline-dark\">See list</Button>\n            </Link>\n          </div>\n        );\n      })}\n    </div>\n  );\n}\n","/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/pages/Explore/Explore.tsx",[],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/store/profiles/selectors.ts",[],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/store/profiles/reducer.ts",["193","194"],"import {\n  ProfileState,\n  ProfileActionTypes,\n  SET_LOADING,\n  PROFILES_FETCHED,\n} from \"./types\";\n\nconst initialState: ProfileState = {\n  loading: true,\n  all: [],\n};\n\nexport default function reducer(\n  state = initialState,\n  action: ProfileActionTypes\n) {\n  switch (action.type) {\n    case \"SET_LOADING\": {\n      return {\n        ...state,\n        loading: action.payload,\n      };\n    }\n    case \"PROFILES_FETCHED\": {\n      return {\n        ...state,\n        all: [...action.payload],\n      };\n    }\n    default: {\n      return state;\n    }\n  }\n}\n","/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/pages/Reviews/Reviews.tsx",[],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/components/LibraryDetails/LibraryDetails.tsx",["195","196"],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/store/apiItems/actions.ts",["197","198","199","200","201","202"],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/components/WishlistDetails/WishlistDetails.tsx",[],"/