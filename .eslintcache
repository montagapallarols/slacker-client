ishlistItemDetails/WishlistItemDetails\";\nimport { selectToken, selectUser } from \"./store/user/selectors\";\nimport { getUserWithStoredToken } from \"./store/user/actions\";\n\nfunction App() {\n  const dispatch = useDispatch();\n  const isLoading = useSelector(selectAppLoading);\n  const user = useSelector(selectUser);\n  const userWithToken = useSelector(selectToken);\n\n  useEffect(() => {\n    dispatch(getUserWithStoredToken());\n  }, [dispatch]);\n\n  return (\n    <div className=\"App\">\n      <Navigation />\n      <MessageBox />\n      {isLoading ? <Loading /> : null}\n      <Switch>\n        <Route exact path=\"/\" component={HomePage} />\n        <Route exact path=\"/login\" component={Login} />\n        <Route exact path=\"/signup\" component={SignUp} />\n        <Route exact path=\"/explore\" component={Explore} />\n        <Route exact path=\"/reviews\" component={Reviews} />\n        <Route exact path=\"/my-profile/:userId\" component={MyProfile} />\n        <Route\n          exact\n          path=\"/my-profile/:userId/library/:categoryName/:itemId\"\n          component={LibraryItemDetails}\n        />\n        <Route\n          exact\n          path=\"/my-profile/:userId/wishlist/:categoryName/:itemId\"\n          component={WishlistItemDetails}\n        />\n        <Route\n          exact\n          path=\"/my-profile/:userId/library/:categoryName\"\n          component={LibraryDetails}\n        />\n        <Route\n          exact\n          path=\"/my-profile/:userId/wishlist/:categoryName\"\n          component={WishlistDetails}\n        />\n      </Switch>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/store/rootReducer.ts",[],["190","191"],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/index.tsx",[],["192","193"],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/store/index.ts",[],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/