d));\n  }\n\n  return (\n    <div>\n      <h2>{apiItemDetails.Title}</h2>\n      <p>{apiItemDetails.Year}</p>\n      <p>({apiItemDetails.Type})</p>\n      <p>Directed by {apiItemDetails.Director}</p>\n      <em>\n        <p>{apiItemDetails.Genre}</p>\n      </em>\n      <img src={apiItemDetails.Poster} height=\"250px\" />\n      <p>{apiItemDetails.Plot}</p>\n      {itemInLibrary ? (\n        <Button onClick={onClickRemove} variant=\"outline-dark\">\n          Remove from Library\n        </Button>\n      ) : (\n        <Button onClick={onClickAdd} variant=\"outline-dark\">\n          Add to Library\n        </Button>\n      )}\n      <Button variant=\"outline-dark\">Favourites</Button>\n    </div>\n  );\n}\n","/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/components/WishlistItemDetails/WishlistItemDetails.tsx",["254","255","256","257","258"],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/store/appState/reducer.ts",[],"/Users/macbook/Codaisseur/week8-PORTFOLIO/slacker/slacker-client/src/components/FavouritesItemDetails/FavouritesItemDetails.tsx",["259","260","261","262","263","264","265","266","267","268","269"],"import React, { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport Button from \"react-bootstrap/Button\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { fetchApiItems, fetchApiItemById } from \"../../store/apiItems/actions\";\nimport {\n  addItemToList,\n  removeItemFromLibrary,\n} from \"../../store/listItems/actions\";\nimport {\n  selectApiItemsLoading,\n  selectAllApiItems,\n  selectApiItemDetails,\n} from \"../../store/apiItems/selectors\";\nimport { selectUser } from \"../../store/user/selectors\";\nimport {\n  selectAllCategories,\n  selectAllListItems,\n} from \"../../store/listItems/selectors\";\n\nexport default function FavouritesItemDetails() {\n  const dispatch = useDispatch();\n  const apiItemsLoading = useSelector(selectApiItemsLoad